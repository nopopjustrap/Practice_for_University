	<entry>
		<title>1. Настройка Git и репозитория</title>
		<link rel="alternate" type="text/html" href="https://practiceinfsec.cnii-jest.ru/component/content/article/journal?catid=11"/>
		<published>2025-04-15T17:31:15+00:00</published>
		<updated>2025-04-15T17:31:15+00:00</updated>
		<id>https://practiceinfsec.cnii-jest.ru/component/content/article/journal?catid=11</id>
		<author>
			<name>Admin</name>
		</author>
		<summary type="html">&lt;div class=&quot;feed-description&quot;&gt;&lt;ul style=&quot;margin-top: 0cm;&quot; type=&quot;disc&quot;&gt;
&lt;li class=&quot;MsoNormal&quot; style=&quot;mso-list: l0 level1 lfo1; tab-stops: list 36.0pt;&quot;&gt;
&lt;p class=&quot;MsoNormal&quot;&gt;&lt;span style=&quot;font-size: 10.0pt; line-height: 115%; font-family: 'Segoe UI Emoji',sans-serif; mso-bidi-font-family: 'Segoe UI Emoji';&quot;&gt;✅&lt;/span&gt;&lt;span style=&quot;font-size: 10.0pt; line-height: 115%; font-family: 'Times New Roman',serif;&quot;&gt; &lt;strong&gt;Выполнено:&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;
&lt;ul style=&quot;margin-top: 0cm;&quot; type=&quot;disc&quot;&gt;
